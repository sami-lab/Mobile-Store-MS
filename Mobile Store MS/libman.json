{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.4.1",
      "destination": "wwwroot/lib/twitter-bootstrap/"
    },
    {
      "library": "popper.js@1.16.1",
      "destination": "wwwroot/lib/popper.js/"
    },
    {
      "library": "font-awesome@5.13.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "jspdf@1.5.3",
      "destination": "wwwroot/lib/JSpdf/"
    },
    {
      "library": "signalr.js@2.4.1",
      "destination": "wwwroot/lib/signalr.js/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/dist/browser/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}